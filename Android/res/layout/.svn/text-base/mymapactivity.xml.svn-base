<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:scrollbars="vertical">
	<TextView        
		android:id="@+id/title"
        android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textSize="12sp"
		android:layout_marginTop="2dip"		
        android:text="@string/app_name"/>

	<RelativeLayout
		android:id="@+id/map_layout"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:layout_below="@+id/title"
		android:layout_marginBottom="20dip" 
		android:layout_marginTop="10dip" 
		>		
		<com.google.android.maps.MapView
			android:id="@+id/mapView"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			android:enabled="true"
			android:clickable="true"
			android:apiKey="0dhSzsycBGkgzODsBMuPR6tgfemW1FpBIM6KcLg" />
	</RelativeLayout>
	
	<ImageView 
		android:id="@+id/iv_low"
		android:src="@drawable/circledotblue"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="5dip"		
		android:layout_marginLeft="5dip"		
		android:layout_alignParentBottom="true">
	</ImageView>
	<TextView        
		android:id="@+id/title2"
        android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textSize="12sp"
        android:text="@string/low"
		android:layout_marginBottom="2dip"		
		android:layout_marginLeft="5dip"		
		android:layout_alignParentBottom="true"
		android:layout_toRightOf="@+id/iv_low"
		/>

	<ImageView 
		android:id="@+id/iv_med"
		android:src="@drawable/circledotyellow"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="5dip"		
		android:layout_marginLeft="25dip"		
		android:layout_alignParentBottom="true"
		android:layout_toRightOf="@+id/title2"		
		>
	</ImageView>
	<TextView        
		android:id="@+id/title3"
        android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textSize="12sp"
        android:text="@string/med"
		android:layout_marginBottom="2dip"		
		android:layout_marginLeft="5dip"		
		android:layout_alignParentBottom="true"
		android:layout_toRightOf="@+id/iv_med"
		/>

	<ImageView 
		android:id="@+id/iv_high"
		android:src="@drawable/circledotred"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="5dip"		
		android:layout_marginLeft="25dip"		
		android:layout_alignParentBottom="true"
		android:layout_toRightOf="@+id/title3"		
		>
	</ImageView>
	<TextView        
		android:id="@+id/title4"
        android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textSize="12sp"
        android:text="@string/high"
		android:layout_marginBottom="2dip"		
		android:layout_marginLeft="5dip"		
		android:layout_alignParentBottom="true"
		android:layout_toRightOf="@+id/iv_high"
		/>

		
</RelativeLayout>


